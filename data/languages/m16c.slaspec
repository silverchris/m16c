:ROLC.w d_eaw                      is (op1_7=0x3B & size0=1 & op12_15=0x0A) ... & d_eaw {
	local carryIn = Cflg;
	local carryout = d_eaw >> 15;
	Cflg = carryout:1;
	d_eaw = (d_eaw << 1) | zext(carryIn);
	Sflg = (d_eaw s< 0);
	Zflg = (d_eaw == 0);
}

