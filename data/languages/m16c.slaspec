COND3: "NE/NZ"   is cnd0_2=0x06  { local tmp:1 = !Zflg; export tmp; }
COND3: "PZ"      is cnd0_2=0x07  { local tmp:1 = !Sflg; export tmp; }

macro macro_adc8(src, dest) {
	local tmp = dest + src + Cflg;
	Cflg = (tmp > 255);
	Zflg = (tmp == 0);
	Sflg = (tmp s< 0);
	Oflg = (tmp s< -128) | (tmp s> 127);
	dest = tmp;
}

macro macro_adc16(src, dest) {
	local c:2 = zext(Cflg);
	Cflg = carry(dest, src) || carry(src+dest, zext(c));
	Oflg = scarry(dest, src) || scarry(src+dest, zext(c));
	dest = src+dest+c;
	Zflg = (dest == 0);
	Sflg = (dest s< 0);
}

macro macro_add(src, dest) {
	Cflg = carry(dest, src);
	Oflg = scarry(dest, src);
	dest = dest+src;
	Zflg = (dest == 0);
	Sflg = (dest s< 0);
}

macro macro_and (src, dest) {
	dest = dest & src;
	Sflg = (dest s< 0);
	Zflg = (dest == 0);
}

macro macro_bm(cond, bit, dest) {
	if (cond == 1) goto <true>;
	dest = dest & ~(1 << zext(bit));
	goto <end>;
	<true>
	dest = dest | (1 << zext(bit));
	<end>
}

macro macro_cmp(src, dest) {
	local tmp = dest - src;
	Cflg = (dest >= src);
	Zflg = (dest == src);
	Sflg = (tmp s< 0);
	Oflg = sborrow(dest, src);
}

macro macro_mov(src, dest) {
	dest = src;
	Sflg = (dest s< 0);
	Zflg = (dest == 0);
}

macro macro_or(src, dest) {
	dest = dest | src;
	Sflg = (dest s< 0);
	Zflg = (dest == 0);
}

macro macro_sbb8(src, dest) {
	local tmp = dest-(src-Cflg);
	Cflg = (tmp s>= 0);
	Zflg = (tmp == 0);
	Sflg = (tmp s< 0);
	Oflg = (tmp s< -128) | (tmp s> 127);
	dest = tmp;
}

macro macro_sbb16(src, dest) {
	local tmp = dest-(src+zext(Cflg));
	Cflg = (tmp s>= 0);
	Zflg = (tmp == 0);
	Sflg = (tmp s< 0);
	Oflg = (tmp s< -32768) | (tmp s> 32767);
	dest = tmp;
}

macro macro_sub(src, dest) {
	local tmp = dest - src;
	Cflg = (tmp s>= 0);
	Zflg = (tmp == 0);
	Sflg = (tmp s< 0);
	Oflg = sborrow(dest, src);
	dest = tmp;
}

