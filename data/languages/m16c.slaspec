COND4: "GE"      is cnd8_11=0x0A { local tmp:1 = (Oflg == Sflg); export tmp; }
COND4: "GT"      is cnd8_11=0x0C { local tmp:1 = !Zflg && (Oflg == Sflg); export tmp; }
COND4: "NO"      is cnd8_11=0x0D { local tmp:1 = !Oflg; export tmp; }
