:RORC.w d_eaw                      is (op1_7=0x3B & size0=1 & op12_15=0x0B) ... & d_eaw {
	local carryOut = Cflg << 15;
	local carryflag = d_eaw&1;
	Cflg = carryflag:1;
	d_eaw = (d_eaw >> 1) | zext(carryOut);
	Sflg = (d_eaw s< 0);
	Zflg = (d_eaw == 0);
}


