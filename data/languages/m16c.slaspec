:EXITD                             is op0_15=0xF27D {
	SP = FB;
	FB = *[near]:2 SP;
	SP = SP+2;
	local return_addr:3 = *[near]:2 SP;
	SP = SP+2;
	local pc_h:3 = zext(*[near]:2 SP) << 16;
	SP = SP-1;
	return_addr = return_addr & pc_h;
	return [return_addr];
}

